{
    "name": "aria-autocomplete",
    "version": "1.0.0-beta.1",
    "description": "Accessible, extensible, dependency-free JavaScript autocomplete",
    "main": "src/aria-autocomplete.js",
    "scripts": {
        "dev": "NODE_ENV=development parcel build src/aria-autocomplete.js --out-file aria-autocomplete.js --no-minify",
        "build": "parcel build src/aria-autocomplete.js --out-file aria-autocomplete.min.js",
        "css": "NODE_ENV=development parcel build src/aria-autocomplete.less --out-file aria-autocomplete.css --no-minify"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mynamesleon/aria-autocomplete.git"
    },
    "keywords": [
        "autocomplete",
        "typeahead",
        "a11y",
        "accessible",
        "accessibility"
    ],
    "author": "Leon Slater (https://www.mynamesleon.com)",
    "license": "GPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/mynamesleon/aria-autocomplete/issues"
    },
    "homepage": "https://github.com/mynamesleon/aria-autocomplete#readme",
    "devDependencies": {
        "less": "^3.10.3",
        "parcel-bundler": "^1.12.4"
    }
}
